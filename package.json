{
  "name": "offline-learning-progress",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run db:migrate && run vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "npm run db:migrate npm run build && vite preview",
    "deploy": "npm run build && npx wrangler deploy",
    "db:migrate": "npx wrangler d1 migrations apply prod-d1-offline-learning-progress --local",
    "db:migrate:remote": "npx wrangler d1 migrations apply prod-d1-offline-learning-progress --remote",
    "predeploy": "npm run db:migrate:remote"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "hono": "^4.10.2",
    "idb": "^8.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.15",
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "wrangler": "^4.45.0"
  }
}
